N#define rt_list_entry(node, type, member) \
N    ((type *)((char *)(node) - (unsigned long)(&((type *)0)->member)))
X#define rt_list_entry(node, type, member)     ((type *)((char *)(node) - (unsigned long)(&((type *)0)->member)))
