Nvoid rt_enter_critical(void)
N{
N    register rt_base_t level;
N
N    /* disable interrupt */
N    level = rt_hw_interrupt_disable();
N
N    /*
N     * the maximal number of nest is RT_UINT16_MAX, which is big
N     * enough and does not check here
N     */
N    rt_scheduler_lock_nest ++;
N
N    /* enable interrupt */
N    rt_hw_interrupt_enable(level);
N}
NRTM_EXPORT(rt_enter_critical);
X;
N
N/**
N * This function will unlock the thread scheduler.
N */
Nvoid rt_exit_critical(void)
N{
N    register rt_base_t level;
N
N    /* disable interrupt */
N    level = rt_hw_interrupt_disable();
N
N    rt_scheduler_lock_nest --;
N
N    if (rt_scheduler_lock_nest <= 0)
N    {
N        rt_scheduler_lock_nest = 0;
N        /* enable interrupt */
N        rt_hw_interrupt_enable(level);
N
N        rt_schedule();
N    }
N    else
N    {
N        /* enable interrupt */
N        rt_hw_interrupt_enable(level);
N    }
N}
NRTM_EXPORT(rt_exit_critical);
X;
N
N/**
N * Get the scheduler lock level
N *
N * @return the level of the scheduler lock. 0 means unlocked.
N */
Nrt_uint16_t rt_critical_level(void)
N{
N    return rt_scheduler_lock_nest;
N}
NRTM_EXPORT(rt_critical_level);
